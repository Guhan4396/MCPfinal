{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "buildCommand": "bash ./build.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/calculate",
      "dest": "/api/calculate"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}

