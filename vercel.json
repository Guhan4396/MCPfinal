{
  "version": 2,
  "builds": [
    {
      "src": "requirements.txt",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/calculate",
      "dest": "/api/calculate"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}

