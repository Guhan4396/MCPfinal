{
  "buildCommand": "next build",
  "devCommand": "next dev",
  "installCommand": "pnpm install && pip install -r requirements.txt",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "rewrites": [
    {
      "source": "/api/calculate",
      "destination": "/api/calculate"
    }
  ],
  "functions": {
    "app/api/calculate/route.ts": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "env": {
    "PYTHON_VERSION": "3.9"
  }
}

